{
  "name": "showcase",
  "version": "0.0.1",
  "description": "My showcase module",
  "devDependencies": {
    "express": "~4.13.3",
    "express-http-proxy": "~0.6.0",
    "gulp": "~3.9.0",
    "gulp-useref": "~1.3.0",
    "gulp-util": "~3.0.6",
    "karma-coverage": "~0.5.0",
    "karma-jasmine": "~0.3.6",
    "karma-junit-reporter": "~0.3.3",
    "karma-phantomjs-launcher": "~0.2.1",
    "wiredep": "~3.0.0-beta"
  },
  "config": {
    "backend-port": "8080",
    "express-port": "8081",
    "backend-server-name": "gumartinm.name",
    "backend-server-url": "showcase"
  },
  "scripts": {
    "prestart": "wiredep -s app/index.html",
    "pretest": "wiredep -s karma.conf.js --dependencies --devDependencies",
    "test": "karma start --reporters progress,junit,coverage",
    "postinstall": "bower install --force-latest && wiredep -s app/index.html"
  }
}
